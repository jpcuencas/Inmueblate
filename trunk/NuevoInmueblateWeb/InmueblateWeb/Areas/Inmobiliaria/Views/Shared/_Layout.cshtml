<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title</title>
    <link rel="shortcut icon" href="@Url.Content("~/Content/favicon.png")"  />
    <link href="@Url.Content("~/Content/bootstrap.min.css")" rel="Stylesheet" type="text/css" />
    @*<link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css" />*@
    <link href="@Url.Content("~/Content/estilos.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/themes/base/jquery.ui.all.css")" rel="stylesheet" type="text/css" />

    <script src="@Url.Content("~/Scripts/jquery-2.1.0.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery-ui-1.10.4.min.js")" type="text/javascript"></script>
     @*<script src="@Url.Content("~/Scripts/modernizr-2.7.2.min.js")" type="text/javascript"></script>*@
    <script src="@Url.Content("~/Scripts/formToWizard.js")" type="text/javascript"></script>

    <script src="https://maps.google.com/maps/api/js?sensor=false"></script>

    @RenderSection("head", false)
</head>
<body>
  
    <div class="navbar navbar-default navbar-fixed-top">
        @Html.Partial("~/Areas/Inmobiliaria/Views/Shared/_Layout_Header.cshtml")
    </div>
    <div class="container">
         <div class="navbar navbar-default col-md-2 menuLateral" role="navigation">
             @Html.Partial("~/Areas/Inmobiliaria/Views/Shared/_Layout_Menu_Lateral.cshtml")
         </div>
    
        <div id="cuerpo">
            <div class="col-md-8 cuerpo-perfil">
                <div class="well bs-component">
                    @RenderBody()
                </div>
            </div>
             <div class="col-md-2 cuerpo-perfil">
                <div class="well bs-component">
                     @Html.Partial("~/Areas/Inmobiliaria/Views/Shared/_Layout_Aside_Anuncios.cshtml")
                </div>
            </div>
        </div>
    </div>
        
    <div class="navbar navbar-default">
        @Html.Partial("~/Areas/Inmobiliaria/Views/Shared/_Layout_Footer.cshtml")
    </div>
</body>
</html>
